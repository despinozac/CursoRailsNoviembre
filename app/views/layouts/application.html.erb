<!DOCTYPE html>
<html>
  <head>
  <title>Curso</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  
  <%= csrf_meta_tags %>
  </head>
  <body>
    <% flash.each do |type, message| %>
      <%= content_tag :div, message, class: type %>
    <% end %>
    <%= yield %>

    <%= javascript_include_tag 'application' %>
  </body>
</html>
